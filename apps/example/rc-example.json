{
    "apiVersion": "v1",
        "kind": "ReplicationController",
        "metadata": {
            "name": "example-rc"
        },
        "spec": {
            "replicas": 1,
            "template": {
                "metadata": {
                    "labels": {
                        "app": "example"
                    }
                },
                "spec": {
                    "containers": [
                    {
                        "name": "example",
                        "image": "nginx",
                        "ports": [
				{"containerPort": 80}
                        ]
                    }
                    ]
                }
            }
        }
}
